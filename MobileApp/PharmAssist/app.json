{
  "expo": {
    "name": "PharmAssist",
    "description": "A very interesting project.",
    "slug": "PharmAssist",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/pill.png",
    "splash": {
      "image": "./assets/images/pill.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "notification":{
      "icon": "./assets/images/pill.png",
      "color": "#54B864"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "TeamOTC"
    },
    "android":{
      "package":"com.android.teamOTC.pharmAssist"
    }
  }
}
